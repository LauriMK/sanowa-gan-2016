            <string>/// Input &amp;&amp; Movement


var keyLeft, keyRight, keyUp, keyDown, keyShoot;

keyLeft = keyboard_check(vk_left) || gamepad_axis_value(0, gp_axislh) &lt; -0.50 || gamepad_button_check(0, gp_padl);
keyRight = keyboard_check(vk_right) || gamepad_axis_value(0, gp_axislh) &gt;  0.50 || gamepad_button_check(0, gp_padr);
keyUp = keyboard_check(vk_up) || gamepad_axis_value(0, gp_axislv) &lt; -0.50 || gamepad_button_check(0, gp_padu);
keyDown = keyboard_check(vk_down) || gamepad_axis_value(0, gp_axislv) &gt;  0.50 || gamepad_button_check(0, gp_padd); 
keyShoot = keyboard_check(vk_control) || gamepad_button_check(0, gp_face1);


if(keyLeft){

    direction += PLAYER_ROTATION;

}

if(keyRight){

    direction -= PLAYER_ROTATION;
}


if(keyUp){

    if(!keyShoot){
    
       speed = PLAYER_SPEED_RUN;
    }
    else{
        
        speed = PLAYER_SPEED_SHOOT;
    }
}
else if(keyDown){

    speed = -PLAYER_SPEED_BACKWARDS;
}

</string>
