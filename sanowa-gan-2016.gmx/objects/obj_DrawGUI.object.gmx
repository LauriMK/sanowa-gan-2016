            <string>//piirretään pysyvät UI osat 

    //healthbarin osat
draw_healthbar(11, 10, 240, 40, obj_Player.playerHitPoints, c_gray, c_red, c_red, 0, false, false)
draw_sprite(spr_Health,0,5,5);

    //powerupin nuoli &amp; seuraavan laatikko
draw_sprite(spr_PowerUpArrow,0,340,10);
draw_sprite(spr_NextBgr, 0, 360, 4);

    //tekstit, healtti ja ammusten määrä
draw_text(0+20,47,obj_Player.magazine);
draw_text(100, 17, obj_Player.playerHitPoints);


//Mikä poweruppi on otettu taskuun

if(obj_Player.powerUpSlot == POWERUP_SLOT_SPREAD){

    draw_sprite(spr_PwrSpread, 0, 364, 0+8); 
}
else if(obj_Player.powerUpSlot == POWERUP_SLOT_SHOTGUN){

    draw_sprite(spr_PwrShotgun2, 0, 364, 0+8); 
}
else if(obj_Player.powerUpSlot == POWERUP_SLOT_GRENADE){

    draw_sprite(spr_PwrGrenade, 0, 364, 0+8); 
}

//Mikä poweruppi on käytössä

if(obj_Player.currentWeapon == WEAPON_SHOTGUN){
    
    draw_sprite(spr_PwrShotgun2, 0, 264, 0+8);
           
}
else if(obj_Player.currentWeapon == WEAPON_GRENADE){

    draw_sprite(spr_PwrGrenade, 0, 264, 0+8);
        
}
else if(obj_Player.currentWeapon == WEAPON_SPREAD){

    draw_sprite(spr_PwrSpread, 0, 264, 0+8);
         
}


//Piirretään lipas spriteinä

var numCols = 21;
var numRows = obj_Player.magazine / numCols;
var bulletsDrawn = 0;
var xOff = 0;
var yOff = 0;

for ( var i = 0; i &lt; numRows; i++){
    
    for (var j = 0; j &lt; numCols; j++){
        
        if(bulletsDrawn == obj_Player.magazine)
            break;
            
            //tähän väliin if kysely eri spriteille
            if(obj_Player.currentWeapon == WEAPON_BASIC || obj_Player.currentWeapon == WEAPON_SPREAD){
                xOff = j * sprite_get_width(spr_Clip);
                yOff = i * sprite_get_height(spr_Clip);
                draw_sprite(spr_Clip, 0, 20 + xOff , 65 + yOff);
            }
            else if(obj_Player.currentWeapon == WEAPON_SHOTGUN){
                xOff = j * sprite_get_width(spr_ClipShtgn);
                yOff = i * sprite_get_height(spr_ClipShtgn);
                draw_sprite(spr_ClipShtgn, 0, 20 + xOff , 65 + yOff);
            }
            else if(obj_Player.currentWeapon == WEAPON_GRENADE){
                xOff = j * sprite_get_width(spr_ClipGrnd);
                yOff = i * sprite_get_height(spr_ClipGrnd);
                draw_sprite(spr_ClipGrnd, 0, 20 + xOff , 65 + yOff);
            }
            //tähän loppuu kysely
            
        bulletsDrawn++;
        
    }
}



//Piirretään  käytössä olevan powerupin laatikko
//tää pitää olla viimesenä että toimii oikein eli piirtyy powerupin päälle

draw_sprite(spr_CurrentBgr, 0, 260, 5);




