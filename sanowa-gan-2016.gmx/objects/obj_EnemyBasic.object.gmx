            <kind>1</kind>
            <string>/// Collision with player

obj_Player.playerHitPoints -= 0.5;

var dir = point_direction(x,y,other.x,other.y);
x = x + lengthdir_x(-5,dir);
y = y + lengthdir_y(-5,dir);

with(other){

    if(place_empty(x+lengthdir_x(-1,point_direction(x,y,other.x,other.y)),y+lengthdir_y(-1,point_direction(x,y,other.x,other.y)))){

        var dir = point_direction(x,y,other.x,other.y);
        x = x + lengthdir_x(-1,dir);
        y = y + lengthdir_y(-1,dir);

    }

}
if(!audio_is_playing(snd_HurtHit1) &amp;&amp; !audio_is_playing(snd_HurtHit2) &amp;&amp; !audio_is_playing(snd_HurtHit3)){

    var num1 = irandom_range(1,3);
    if(num1 == 1){
        audio_play_sound(snd_HurtHit1, 2, false);
    }
    else if (num1 == 2){
        audio_play_sound(snd_HurtHit2, 2, false);
    }
    else{
        audio_play_sound(snd_HurtHit3, 2, false);
    }
}
if(!audio_is_playing(snd_EnemyBite1) &amp;&amp; !audio_is_playing(snd_EnemyBite2) &amp;&amp; !audio_is_playing(snd_EnemyBite3)){

