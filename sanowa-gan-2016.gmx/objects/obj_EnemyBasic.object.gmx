        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Collision with shells

if(other.enemyID != self.id){

    if(distance_to_point(obj_Player.x,obj_Player.y) &gt; 500){ 
    
        self.hitPoints -= 5;    
        other.hitPoints -= 100;     
    }
    
    else if (distance_to_point(obj_Player.x,obj_Player.y) &gt;= 300) &amp;&amp; (distance_to_point(obj_Player.x,obj_Player.y) &lt;= 500) {
        
        self.hitPoints -= 17.5;
        other.hitPoints -= 45;
    }
    
    else {
    
        self.hitPoints -= 25;
        other.hitPoints -= 34;
    
    }
    
    
    
    //knockback
    self.backwards = true;
    
    if(distance_to_point(obj_Player.x,obj_Player.y) &lt;=350) {
        speed = -20; 
        alarm[0]= room_speed * 0.08;
    }
    else {
        speed = -5;
        alarm[0] = room_speed * 0.1;
    }  
    
    //hit effects
    var dir = point_direction(x,y,other.x,other.y);
    effect_create_above(ef_flare, x+lengthdir_x(10,dir), y+lengthdir_y(10,dir), 0, c_orange); 
    
    
}

other.enemyID = self.id;
</string>
